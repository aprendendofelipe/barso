{
  "name": "@tabnews/config",
  "version": "0.3.1",
  "description": "Configurações padronizadas dos projetos TabNews",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./eslint": "./src/shared/eslint.config.cjs",
    "./lint-staged": "./src/shared/lint-staged.cjs",
    "./package.json": "./package.json",
    "./prettier": "./src/shared/prettier.config.cjs",
    "./vitest": "./src/shared/vitest.config.js"
  },
  "bin": {
    "tn": "bin/tn.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "tn": "tn",
    "test": "tn test",
    "test:run": "tn test run",
    "lint": "eslint . --max-warnings 0 && prettier --check .",
    "format": "eslint --fix . && prettier --write .",
    "commit": "cz",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@eslint/js": "9.20.0",
    "@next/eslint-plugin-next": "15.1.7",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.0.5",
    "commander": "13.1.0",
    "dotenv": "16.4.7",
    "dotenv-expand": "12.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-primer-react": "6.1.6",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-vitest": "0.5.4",
    "eslint-plugin-vitest-globals": "1.5.0",
    "globals": "15.14.0",
    "jsdom": "26.0.0",
    "prettier": "3.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@tabnews/config": "file:",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3"
  },
  "author": "Turma do TabNews",
  "repository": "aprendendofelipe/tabnews-config",
  "keywords": [
    "cli",
    "config",
    "tabnews"
  ]
}
